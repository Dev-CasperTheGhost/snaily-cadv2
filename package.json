    "CAD/MDT",
    "CAD",
    "mdt",
    "cad",
    "snailycad",
    "snaily-cad",
    "SnailyCAD",
    "snailyCAD"
  ],
  "author": "Dev-CasperTheGhost",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/Dev-CasperTheGhost/snaily-cadv2/issues"
  },
  "homepage": "https://github.com/Dev-CasperTheGhost/snaily-cadv2#readme",
  "dependencies": {
    "concurrently": "^5.2.0"
  },
  "devDependencies": {
    "nodemon": "^2.0.4"
  }
}
