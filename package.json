{
  "name": "snailycad-v2",
  "version": "1.2.0",
  "description": "SnailyCAD is a Free and secure NodeJS CAD/MDT for your community!",
  "main": "config.js",
  "scripts": {
    "server": "npm start --prefix server",
    "client": "npm run serve --prefix client",
    "start": "concurrently \"npm run server\" \"npm run client\"",
    "create": "npm install --prefix client && npm install --prefix server"
  },
